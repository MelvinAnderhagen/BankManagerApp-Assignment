@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">You do not have access to this resource. Please <a asp-page="/Areas/Identity/Pages/Account/Login">log in</a> to see the page.</p>
</header>
