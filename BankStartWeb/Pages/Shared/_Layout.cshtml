<section class="header  navigation bg-white border-radius-for-nav">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <nav class="navbar navbar-expand-lg bg-white">
                    <a class="navbar-brand" asp-page="/Index">
                        <h2>Bank AB</h2>
                    </a>
                    <button class="navbar-toggler text-dark" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="tf-ion-android-menu"></span>
                    </button>
                    <div class="collapse navbar-collapse text-dark" id="navbarSupportedContent">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item active">
                                <a class="nav-link text-dark" asp-page="/Index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-page="/Privacy">Privacy</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-page="/Bank/Customers/Customers">Customers</a>
                            </li>
                        </ul>

                    </div>
                    <partial name="_LoginPartial"/>
                </nav>

            </div>
        </div>
    </div>
</section>


<div class="container testbody">
    <main role="main" class="pb-3">
        <partial name="_ToastrNotification"/>
        @RenderBody()
    </main>
</div>

<footer class="position-sticky mt-auto">
    <div class="top-footer mt-auto">
        <div class="container mt-auto">
            <div class="row mt-auto">
                <div class="col-sm-3 col-md-3 col-lg-3 mt-auto">
                    <h3>INFORMATION</h3>
                    <ul>
                        <li><a href="#">Payment Option</a></li>
                        <li><a href="#">Free Schedule</a></li>
                        <li><a href="#">Getting Started</a></li>
                    </ul>
                </div>
                <!-- End of .col-sm-3 -->

                <div class="col-sm-3 col-md-3 col-lg-3 mt-auto">
                    <h3>Our Services</h3>
                    <ul>
                        <li><a href="#">Graphic Design</a></li>
                        <li><a href="#">Web Design</a></li>
                        <li><a href="#">Web Development</a></li>
                    </ul>
                </div>
                <!-- End of .col-sm-3 -->

                <div class="col-sm-3 col-md-3 col-lg-3">
                    <h3>Quick Links</h3>
                    <ul>
                        <li><a asp-page="/Bank/Customers/Customers">Customers</a></li>
                        <li><a asp-page="/Privacy">Privacy Policy</a></li>
                    </ul>
                </div>
                <!-- End of .col-sm-3 -->

                <div class="col-sm-3 col-md-3 col-lg-3">
                    <h3>Follow Us</h3>
                    <ul>
                        <li>
                            <a href="https://www.linkedin.com/in/melvin-anderhagen-2195131b3/">
                                Linkedin
                            </a>
                        </li>
                        <li>
                            <a href="https://github.com/MelvinAnderhagen">
                                Github
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- End of .col-sm-3 -->

            </div>
        </div> <!-- end container -->
    </div>
    <div class="footer-bottom">
        <h5>Copyright 2021. All rights reserved.</h5>
        <h6>Backend and Some Design Developed by <a href="https://github.com/MelvinAnderhagen">Melvin Anderhagen</a></h6>
    </div>
</footer> <!-- end footer -->

